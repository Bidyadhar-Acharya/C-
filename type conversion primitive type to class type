//TYPE-CONVERSION  (1) PIMITIVE TYPE TO CLASS TYPE
// PRIMITIVE TYPE TO CLASS TYPE IS DONE WITH THE HELP OF CONSTRUCTORS
#include<iostream.h>
#include<conio.h>
class complex
{
	int a,b;
public:
	complex()
	{
		cout<<"\n DEFAULT CO0NSTRUCTOR CALLED";
	}
	complex(int x)
	{
		a=x;
		b=0;
	}
	void getdata(int x, int y)
	{
		a=x;
		b=y;
	}
	void showdata()
	{
		cout<<endl<<"a= "<<a<<" b= "<<b;
	}
};
void main()
{
	complex c1;
	clrscr();
	int x=5;
	c1=x;
	c1.showdata();

	getch();
}
